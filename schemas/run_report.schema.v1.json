{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "run_report schema v1",
  "type": "object",
  "required": [
    "run_report_schema_version",
    "run_id",
    "selection"
  ],
  "properties": {
    "run_report_schema_version": {
      "type": "integer"
    },
    "run_id": {
      "type": "string"
    },
    "flags": {
      "type": "object",
      "properties": {
        "min_score": {
          "type": "integer"
        },
        "min_alert_score": {
          "type": "integer"
        }
      },
      "additionalProperties": true
    },
    "selection": {
      "type": "object",
      "required": [
        "scrape_provenance",
        "classified_job_count",
        "classified_job_count_by_provider"
      ],
      "properties": {
        "scrape_provenance": {
          "type": "object"
        },
        "classified_job_count": {
          "type": "integer"
        },
        "classified_job_count_by_provider": {
          "type": "object",
          "additionalProperties": {
            "type": "integer"
          }
        }
      },
      "additionalProperties": true
    },
    "scoring_input_selection_by_profile": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "required": [
          "selected_path",
          "candidate_paths_considered",
          "selection_reason"
        ],
        "properties": {
          "selected_path": {
            "type": [
              "string",
              "null"
            ]
          },
          "candidate_paths_considered": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "path",
                "exists"
              ],
              "properties": {
                "path": {
                  "type": "string"
                },
                "exists": {
                  "type": "boolean"
                },
                "mtime_iso": {
                  "type": [
                    "string",
                    "null"
                  ]
                },
                "sha256": {
                  "type": [
                    "string",
                    "null"
                  ]
                }
              },
              "additionalProperties": true
            }
          },
          "selection_reason": {
            "type": "string",
            "enum": [
              "ai_only",
              "no_enrich_enriched_newer",
              "no_enrich_labeled_newer_or_equal",
              "no_enrich_enriched_only",
              "no_enrich_labeled_only",
              "no_enrich_missing",
              "default_enriched_required",
              "default_enriched_missing",
              "prefer_ai_enriched"
            ]
          },
          "comparison_details": {
            "type": "object",
            "properties": {
              "newer_by_seconds": {
                "type": "number"
              },
              "prefer_ai": {
                "type": "boolean"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": true
      }
    },
    "scoring_input_selection_by_provider": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "required": [
            "selected_path",
            "candidate_paths_considered",
            "selection_reason"
          ],
          "properties": {
            "selected_path": {
              "type": [
                "string",
                "null"
              ]
            },
            "candidate_paths_considered": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "path",
                  "exists"
                ],
                "properties": {
                  "path": {
                    "type": "string"
                  },
                  "exists": {
                    "type": "boolean"
                  },
                  "mtime_iso": {
                    "type": [
                      "string",
                      "null"
                    ]
                  },
                  "sha256": {
                    "type": [
                      "string",
                      "null"
                    ]
                  }
                },
                "additionalProperties": true
              }
            },
            "selection_reason": {
              "type": "string",
              "enum": [
                "ai_only",
                "no_enrich_enriched_newer",
                "no_enrich_labeled_newer_or_equal",
                "no_enrich_enriched_only",
                "no_enrich_labeled_only",
                "no_enrich_missing",
                "default_enriched_required",
                "default_enriched_missing",
                "prefer_ai_enriched"
              ]
            },
            "comparison_details": {
              "type": "object",
              "properties": {
                "newer_by_seconds": {
                  "type": "number"
                },
                "prefer_ai": {
                  "type": "boolean"
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": true
        }
      }
    },
    "publish": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "required": {
          "type": "boolean"
        },
        "bucket": {
          "type": [
            "string",
            "null"
          ]
        },
        "prefix": {
          "type": [
            "string",
            "null"
          ]
        },
        "pointer_write": {
          "type": "object",
          "properties": {
            "global": {
              "type": "string"
            },
            "provider_profile": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "error": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": true
    },
    "delta_summary": {
      "type": "object"
    }
  },
  "additionalProperties": true
}
