{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "run_report schema v1",
  "type": "object",
  "required": [
    "run_report_schema_version",
    "run_id",
    "selection",
    "verifiable_artifacts",
    "config_fingerprint",
    "environment_fingerprint"
  ],
  "properties": {
    "run_report_schema_version": {
      "type": "integer"
    },
    "run_id": {
      "type": "string"
    },
    "flags": {
      "type": "object",
      "properties": {
        "min_score": {
          "type": "integer"
        },
        "min_alert_score": {
          "type": "integer"
        }
      },
      "additionalProperties": true
    },
    "selection": {
      "type": "object",
      "required": [
        "scrape_provenance",
        "classified_job_count",
        "classified_job_count_by_provider"
      ],
      "properties": {
        "scrape_provenance": {
          "type": "object"
        },
        "classified_job_count": {
          "type": "integer"
        },
        "classified_job_count_by_provider": {
          "type": "object",
          "additionalProperties": {
            "type": "integer"
          }
        }
      },
      "additionalProperties": true
    },
    "ai_accounting": {
      "type": "object",
      "required": [
        "model_usage",
        "totals"
      ],
      "properties": {
        "model_usage": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "model",
              "calls",
              "tokens_in",
              "tokens_out",
              "tokens_total",
              "estimated_cost_usd"
            ],
            "properties": {
              "model": {
                "type": "string"
              },
              "calls": {
                "type": "integer",
                "minimum": 0
              },
              "tokens_in": {
                "type": "integer",
                "minimum": 0
              },
              "tokens_out": {
                "type": "integer",
                "minimum": 0
              },
              "tokens_total": {
                "type": "integer",
                "minimum": 0
              },
              "estimated_cost_usd": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "totals": {
          "type": "object",
          "required": [
            "calls",
            "tokens_in",
            "tokens_out",
            "tokens_total",
            "estimated_cost_usd"
          ],
          "properties": {
            "calls": {
              "type": "integer",
              "minimum": 0
            },
            "tokens_in": {
              "type": "integer",
              "minimum": 0
            },
            "tokens_out": {
              "type": "integer",
              "minimum": 0
            },
            "tokens_total": {
              "type": "integer",
              "minimum": 0
            },
            "estimated_cost_usd": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "scoring_model": {
      "type": "object",
      "required": [
        "version",
        "algorithm_id",
        "config_sha256",
        "module_path",
        "code_sha256",
        "inputs"
      ],
      "properties": {
        "version": {
          "type": "string"
        },
        "algorithm_id": {
          "type": "string"
        },
        "config_sha256": {
          "type": "string"
        },
        "module_path": {
          "type": "string"
        },
        "code_sha256": {
          "type": "string"
        },
        "inputs": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "pointer_type",
              "path",
              "sha256"
            ],
            "properties": {
              "pointer_type": {
                "type": "string"
              },
              "provider": {
                "type": "string"
              },
              "profile": {
                "type": "string"
              },
              "path": {
                "type": "string"
              },
              "sha256": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "scoring_input_selection_by_profile": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "required": [
          "selected_path",
          "candidate_paths_considered",
          "selection_reason"
        ],
        "properties": {
          "selected_path": {
            "type": [
              "string",
              "null"
            ]
          },
          "candidate_paths_considered": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "path",
                "exists"
              ],
              "properties": {
                "path": {
                  "type": "string"
                },
                "exists": {
                  "type": "boolean"
                },
                "mtime_iso": {
                  "type": [
                    "string",
                    "null"
                  ]
                },
                "sha256": {
                  "type": [
                    "string",
                    "null"
                  ]
                }
              },
              "additionalProperties": true
            }
          },
          "selection_reason": {
            "type": "string",
            "enum": [
              "ai_only",
              "no_enrich_enriched_newer",
              "no_enrich_labeled_newer_or_equal",
              "no_enrich_enriched_only",
              "no_enrich_labeled_only",
              "no_enrich_missing",
              "default_enriched_required",
              "default_enriched_missing",
              "prefer_ai_enriched"
            ]
          },
          "comparison_details": {
            "type": "object",
            "properties": {
              "newer_by_seconds": {
                "type": "number"
              },
              "prefer_ai": {
                "type": "boolean"
              }
            },
            "additionalProperties": false
          },
          "selection_reason_details": {
            "type": "object",
            "properties": {
              "labeled_vs_enriched": {
                "type": "object",
                "required": [
                  "rule_id",
                  "chosen_path",
                  "candidate_paths",
                  "compared_fields",
                  "decision",
                  "decision_timestamp"
                ],
                "properties": {
                  "rule_id": {
                    "type": "string"
                  },
                  "chosen_path": {
                    "type": [
                      "string",
                      "null"
                    ]
                  },
                  "candidate_paths": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "compared_fields": {
                    "type": "object"
                  },
                  "decision": {
                    "type": "string"
                  },
                  "decision_timestamp": {
                    "type": "string"
                  }
                },
                "additionalProperties": true
              },
              "enriched_vs_ai": {
                "type": "object",
                "required": [
                  "rule_id",
                  "chosen_path",
                  "candidate_paths",
                  "compared_fields",
                  "decision",
                  "decision_timestamp"
                ],
                "properties": {
                  "rule_id": {
                    "type": "string"
                  },
                  "chosen_path": {
                    "type": [
                      "string",
                      "null"
                    ]
                  },
                  "candidate_paths": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "compared_fields": {
                    "type": "object"
                  },
                  "decision": {
                    "type": "string"
                  },
                  "decision_timestamp": {
                    "type": "string"
                  }
                },
                "additionalProperties": true
              }
            },
            "additionalProperties": true
          }
        },
        "additionalProperties": true
      }
    },
    "scoring_input_selection_by_provider": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "required": [
            "selected_path",
            "candidate_paths_considered",
            "selection_reason"
          ],
          "properties": {
            "selected_path": {
              "type": [
                "string",
                "null"
              ]
            },
            "candidate_paths_considered": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "path",
                  "exists"
                ],
                "properties": {
                  "path": {
                    "type": "string"
                  },
                  "exists": {
                    "type": "boolean"
                  },
                  "mtime_iso": {
                    "type": [
                      "string",
                      "null"
                    ]
                  },
                  "sha256": {
                    "type": [
                      "string",
                      "null"
                    ]
                  }
                },
                "additionalProperties": true
              }
            },
            "selection_reason": {
              "type": "string",
              "enum": [
                "ai_only",
                "no_enrich_enriched_newer",
                "no_enrich_labeled_newer_or_equal",
                "no_enrich_enriched_only",
                "no_enrich_labeled_only",
                "no_enrich_missing",
                "default_enriched_required",
                "default_enriched_missing",
                "prefer_ai_enriched"
              ]
            },
            "comparison_details": {
              "type": "object",
              "properties": {
                "newer_by_seconds": {
                  "type": "number"
                },
                "prefer_ai": {
                  "type": "boolean"
                }
              },
              "additionalProperties": false
            },
            "selection_reason_details": {
              "type": "object",
              "properties": {
                "labeled_vs_enriched": {
                  "type": "object",
                  "required": [
                    "rule_id",
                    "chosen_path",
                    "candidate_paths",
                    "compared_fields",
                    "decision",
                    "decision_timestamp"
                  ],
                  "properties": {
                    "rule_id": {
                      "type": "string"
                    },
                    "chosen_path": {
                      "type": [
                        "string",
                        "null"
                      ]
                    },
                    "candidate_paths": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "compared_fields": {
                      "type": "object"
                    },
                    "decision": {
                      "type": "string"
                    },
                    "decision_timestamp": {
                      "type": "string"
                    }
                  },
                  "additionalProperties": true
                },
                "enriched_vs_ai": {
                  "type": "object",
                  "required": [
                    "rule_id",
                    "chosen_path",
                    "candidate_paths",
                    "compared_fields",
                    "decision",
                    "decision_timestamp"
                  ],
                  "properties": {
                    "rule_id": {
                      "type": "string"
                    },
                    "chosen_path": {
                      "type": [
                        "string",
                        "null"
                      ]
                    },
                    "candidate_paths": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "compared_fields": {
                      "type": "object"
                    },
                    "decision": {
                      "type": "string"
                    },
                    "decision_timestamp": {
                      "type": "string"
                    }
                  },
                  "additionalProperties": true
                }
              },
              "additionalProperties": true
            }
          },
          "additionalProperties": true
        }
      }
    },
    "archived_inputs_by_provider_profile": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "properties": {
            "selected_scoring_input": {
              "type": "object",
              "required": [
                "source_path",
                "archived_path",
                "sha256",
                "bytes"
              ],
              "properties": {
                "source_path": {
                  "type": "string"
                },
                "archived_path": {
                  "type": "string"
                },
                "sha256": {
                  "type": "string"
                },
                "bytes": {
                  "type": "integer"
                },
                "hash_algo": {
                  "type": "string",
                  "enum": [
                    "sha256"
                  ]
                }
              },
              "additionalProperties": true
            },
            "profile_config": {
              "type": "object",
              "required": [
                "source_path",
                "archived_path",
                "sha256",
                "bytes"
              ],
              "properties": {
                "source_path": {
                  "type": "string"
                },
                "archived_path": {
                  "type": "string"
                },
                "sha256": {
                  "type": "string"
                },
                "bytes": {
                  "type": "integer"
                },
                "hash_algo": {
                  "type": "string",
                  "enum": [
                    "sha256"
                  ]
                }
              },
              "additionalProperties": true
            }
          },
          "additionalProperties": true
        }
      }
    },
    "verifiable_artifacts": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "required": [
          "path",
          "sha256",
          "bytes"
        ],
        "properties": {
          "path": {
            "type": "string"
          },
          "sha256": {
            "type": "string"
          },
          "bytes": {
            "type": "integer"
          },
          "hash_algo": {
            "type": "string",
            "enum": [
              "sha256"
            ]
          }
        },
        "additionalProperties": true
      }
    },
    "config_fingerprint": {
      "type": "string"
    },
    "environment_fingerprint": {
      "type": "object",
      "properties": {
        "python_version": {
          "type": "string"
        },
        "platform": {
          "type": "string"
        },
        "image_tag": {
          "type": [
            "string",
            "null"
          ]
        },
        "git_sha": {
          "type": [
            "string",
            "null"
          ]
        },
        "tz": {
          "type": [
            "string",
            "null"
          ]
        },
        "pythonhashseed": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "additionalProperties": true
    },
    "publish": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "required": {
          "type": "boolean"
        },
        "bucket": {
          "type": [
            "string",
            "null"
          ]
        },
        "prefix": {
          "type": [
            "string",
            "null"
          ]
        },
        "pointer_write": {
          "type": "object",
          "properties": {
            "global": {
              "type": "string"
            },
            "provider_profile": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "error": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": true
    },
    "provenance": {
      "type": "object",
      "properties": {
        "build": {
          "type": "object",
          "required": [
            "git_sha",
            "image",
            "taskdef",
            "ecs_task_arn"
          ],
          "properties": {
            "git_sha": {
              "type": "string"
            },
            "image": {
              "type": "string"
            },
            "taskdef": {
              "type": "string"
            },
            "ecs_task_arn": {
              "type": "string"
            }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": true
    },
    "delta_summary": {
      "type": "object"
    }
  },
  "additionalProperties": true
}
